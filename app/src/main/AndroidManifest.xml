<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.georgicaracu.thehiddencampus" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.georgicaracu.thehiddencampus.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.georgicaracu.thehiddencampus.Register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.georgicaracu.thehiddencampus.MainMenu"
            android:label="@string/title_activity_main_menu" >
        </activity>
        <activity
            android:name="com.example.georgicaracu.thehiddencampus.ViewEvent"
            android:label="@string/title_activity_view_event" >
        </activity>
        <activity
            android:name="com.example.georgicaracu.thehiddencampus.EventList"
            android:label="@string/title_activity_event_list" >
        </activity>
    </application>

</manifest>
